
	<%- include('partials/_header'); %> 
	<%- include('partials/_nav'); %> 
	
	
    <div id="overviews" class="section wb">
        <div class="container">
            <div class="row"> 
                <div class="offset-1 col-lg-10 blog-post-single">
                    <div class="blog-item">
						<div class="image-blog">
							<img style="height: 400px;" src="/images/blog_single.jpg" alt="" class="img-fluid">
						</div>
						<div class="post-content">
							<div class="post-date">
								<span class="day"><%=course.creatAt.getDate() %></span>
								<span  class="month"><%=course.creatAt.getMonth() %></span>
							</div>
							<div class="meta-info-blog">
								<span><i class="fa fa-calendar"></i> <a href="#"><%=course.creatAt.getDate() %>.<%=course.creatAt.getMonth() %>.<%=course.creatAt.getFullYear() %> </a> </span>
								<span><i class="fa fa-tag"></i>  <a href="#">Category</a> </span>
								<span><i class="fa fa-comments"></i> <a href="#">X Teacher</a></span>
							</div>
							<div class="blog-title">
								<h2><a href="#" title=""><%=course.name %> </a></h2>
							</div>
							<div class="blog-desc">
								<p><%=course.description%></p>
								
							</div>							
						</div>
					</div>
                </div><!-- end col -->
				
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end section -->

	<%- include('partials/_footer'); %>